{
  "name": "@infititys/api-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "api-e2e",
    "projectType": "application",
    "implicitDependencies": [
      "api"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{workspaceRoot}/coverage/{e2eProjectRoot}"
        ],
        "options": {
          "jestConfig": "apps/api-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "api:build"
        ]
      }
    }
  }
}
